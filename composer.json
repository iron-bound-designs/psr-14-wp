{
  "name": "ironbound/psr-14-wp",
  "description": "PSR-14 Events Implementation for WordPress",
  "type": "library",
  "require": {
    "psr/event-dispatcher": "^1.0",
    "php": ">=7.2.0"
  },
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Timothy Jacobs",
      "email": "timothy@ironbounddesigns.com"
    }
  ],
  "minimum-stability": "stable",
  "provide": {
    "psr/event-dispatcher-implementation": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "IronBound\\Psr14WP\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "tests/stub.php"
    ]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "johnpbloch/wordpress-core": "^5.3",
    "phpunit/phpunit": "^8.5"
  }
}
